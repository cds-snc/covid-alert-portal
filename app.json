{
  "name": "healthcare-portal",
  "stack": "container",
  "addons": ["heroku-postgresql:hobby-dev"],
  "environments": {
    "review": {
      "env": {
        "DJANGO_DEBUG": "True",
        "DJANGO_ENV": "development",
        "DOCKERFILE_APPJSON": "review app.json",
        "DOCKERFILE_HEROKUYML_APPJSON": "review app.json"
      }
    },
    "staging": {
      "env": {
        "DJANGO_DEBUG": "False",
        "DJANGO_ENV": "production",
        "DOCKERFILE_APPJSON": "staging app.json",
        "DOCKERFILE_HEROKUYML_APPJSON": "staging app.json"
      }
    }
  },
  "env": {
    "DJANGO_DEBUG": "True",
    "DJANGO_ENV": "development",
    "DOCKERFILE_APPJSON": "global app.json",
    "DOCKERFILE_HEROKUYML_APPJSON": "global app.json"
  }
}
